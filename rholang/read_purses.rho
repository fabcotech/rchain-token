new return, entryCh, readCh, lookup(`rho:registry:lookup`) in {
  lookup!(`rho:id:MASTER_REGISTRY_URI`, *entryCh) |
  for(entry <- entryCh) {
    new purse0Ch in {
      entry!(("PUBLIC_READ_PURSE", { "contractId": "CONTRACT_ID", "purseId": PURSE_ID }, *purse0Ch)) |
      for (@purse0 <- purse0Ch) {
        return!({}.set(PURSE_ID, purse0))
      }
    }
  }
}